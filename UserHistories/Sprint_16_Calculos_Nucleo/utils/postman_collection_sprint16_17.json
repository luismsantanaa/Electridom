      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/calculos/proyecto/{{proyectoId}}",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "calculos",
            "proyecto",
            "{{proyectoId}}"
          ]
        }
      }
    },
    {

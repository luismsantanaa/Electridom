    "@types/multer": "^2.0.0",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "helmet": "^8.1.0",
    "mariadb": "^3.4.2",
    "multer": "^2.0.2",
    "nestjs-paginate": "^12.5.0",
    "nestjs-pino": "^4.4.0",
    "openai": "^5.15.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "prom-client": "^15.1.3",
    "puppeteer": "^24.17.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.24",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5"

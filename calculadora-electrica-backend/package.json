{
  "name": "calculadora-electrica-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build --tsc",
    "postbuild": "cp -r src/database/seeds/*.json dist/src/database/seeds/ 2>/dev/null || true",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --watch",
    "test:e2e:cov": "jest --config ./test/jest-e2e.json --coverage",
    "test:calculations": "jest --testPathPattern=calculations",
    "test:calculations:e2e": "jest --config ./test/jest-e2e.json --testPathPattern=calculations.e2e-spec.ts",
    "test:projects": "jest --testPathPattern=projects",
    "test:projects:e2e": "jest --config ./test/jest-e2e.json --testPathPattern=projects.e2e-spec.ts",
    "test:performance": "jest --testPathPattern=performance",
    "test:coverage": "jest --coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=html",
    "setup:test-db": "node scripts/setup-test-db.js",
    "setup:test-db-complete": "ts-node scripts/setup-test-db-complete.ts",
    "cleanup:test-db": "node scripts/cleanup-test-db.js",
    "keys:rotate": "ts-node scripts/rotate-keys.ts",
    "test:e2e:setup": "npm run setup:test-db && npm run test:e2e",
    "test:e2e:clean": "npm run setup:test-db",
    "test:all": "npm run test && npm run test:e2e",
    "test:all:clean": "npm run setup:test-db && npm run test:all",
    "test:unit": "jest --testPathIgnorePatterns=e2e",
    "test:unit:watch": "jest --testPathIgnorePatterns=e2e --watch",
    "test:unit:coverage": "jest --testPathIgnorePatterns=e2e --coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=html",
    "verify:cors": "powershell -ExecutionPolicy Bypass -File scripts/verify-cors-config.ps1",
    "verify:cors:custom": "powershell -ExecutionPolicy Bypass -File scripts/verify-cors-config.ps1 -BackendUrl",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/typeorm.config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/typeorm.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/typeorm.config.ts",
    "seed": "ts-node scripts/run-seeds.ts",
    "migrate": "ts-node -r tsconfig-paths/register scripts/run-migrations.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
